class Sprite {
  void playSound(string sound) {
    __asm__("[\"playSound:\", [\"getParam\", \"sound\"]]");
  };

  void nextCostume() {
    __asm__("[\"nextCostume\"]");
  };
}

class MyCat : Sprite {
  string meowSound;
  int catType;

  void meow() {
    nextCostume();
    playSound("meow");
  };

  void talk(string phrase);
}
